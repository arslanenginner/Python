# This module import the time
import time
times=time.strftime("%H:%M")
times=int(time.strftime('%H'))
if(times<6):
    print("Good Morning sir:",times)
elif(times<=16):
    print("Good Afternoon sir:",times)
elif(times<=19):
    print("Good Night sir:",times)
elif(times<=4):
    print("Good Early morning sir:",times)

